		"vitest": "^1.2.1"
	},
	"type": "module",
	"dependencies": {
		"@flaticon/flaticon-uicons": "^3.1.0",
		"@floating-ui/dom": "1.6.0",
		"chart.js": "^4.4.1",
		"fast-xml-parser": "^4.3.3",
		"monaco-editor": "^0.45.0",
